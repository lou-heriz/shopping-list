{
  "name": "shopping-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:coverage": "set CYPRESS_COVERAGE=true && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:playwright": "playwright test --headed",
    "test": "set LOWDB_PATH=./cypress/db/test-db.json&& start-server-and-test dev http://localhost:3000 cypress:run",
    "test:coverage": "set LOWDB_PATH=./cypress/db/test-db.json&& start-server-and-test dev:coverage http://localhost:3000 \"cypress run && nyc report\" && npm run test:playwright",
    "coverage:report": "nyc report --reporter=html --reporter=text",
    "coverage:open": "start coverage/index.html"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "lowdb": "^7.0.1",
    "motion": "^12.23.12",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@eslint/eslintrc": "^3",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "^14.5.3",
    "cypress-real-events": "^1.14.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "istanbul": "^0.4.5",
    "nyc": "^17.1.0",
    "playwright": "^1.54.1",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
